 else { 
            setCreateDate(actdate); 
            setActivateDate(actdate); 
        } 
         
	   	setDeActivateDate(deactdate); 
        setSalutation(salutation); 
        setIsEnabled(enabled); 
        setUniqueDeviceId(uniqueDeviceId); 
        setIsPrepay(isprepay); 
    } 
 
